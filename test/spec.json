{
    "llvm-target": "i386-unknown-linux-musl",
    "cpu": "x86-64",
    "goos": "linux",
    "goarch": "amd64",
    "build-tags": ["amd64", "linux"],
    "linker": "ld.lld",
    "rtlib": "compiler-rt",
    "libc": "musl",
    "scheduler": "none",
    "gc": "none",
    "defaultstacksize": 65536,
    "ldflags": [
        "--gc-sections",
        "--discard-all",
        "--strip-all",
        "--no-rosegment",
        "-znorelro",
        "-znognustack"
    ]
}
